<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginHorizontal="16dp"
    android:layout_marginBottom="16dp"
    android:visibility="visible">

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/age" />

        <TextView
            android:id="@+id/age"
            style="@style/TextTableResultBody"
            android:text="@string/age_years" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/gender" />

        <TextView
            android:id="@+id/gender"
            style="@style/TextTableResultBody"
            android:text="@string/boy_girl" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/frequent_urination" />

        <TextView
            android:id="@+id/isPolyuria"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/often_feel_thirsty" />

        <TextView
            android:id="@+id/isPolydipsia"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/losing_weight_suddenly" />

        <TextView
            android:id="@+id/isWeightLoss"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/often_feel_weak" />

        <TextView
            android:id="@+id/isWeakness"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/often_feel_hungry" />

        <TextView
            android:id="@+id/isPolyphagia"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/often_feel_itchy_genitals" />

        <TextView
            android:id="@+id/isGenitalThrus"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/often_feel_itchy_in_body" />

        <TextView
            android:id="@+id/isItching"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/easily_get_angry" />

        <TextView
            android:id="@+id/isIrritability"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/difficult_to_heal_or_dry" />

        <TextView
            android:id="@+id/isDelayedHealing"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/experiencing_paralysis_in_part_of_body" />

        <TextView
            android:id="@+id/isPartialParesis"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/stiff_muscles" />

        <TextView
            android:id="@+id/isMuscleStiffness"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/hair_loss" />

        <TextView
            android:id="@+id/isAlopecia"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/experiencing_obesity" />

        <TextView
            android:id="@+id/isObesity"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:text="@string/experiencing_visual_blurring" />

        <TextView
            android:id="@+id/isVisualBlurring"
            style="@style/TextTableResultBody"
            android:text="@string/yes_or_no" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="4dp"
            android:layout_weight="10"
            android:background="@color/black" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <TextView
            style="@style/TextTableResultHeader"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/result_detection"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/isDiabetes"
            style="@style/TextTableResultBody"
            android:fontFamily="@font/poppins_medium"
            android:text="@string/yes_or_no"
            android:textSize="16sp" />
    </TableRow>

    <TableRow style="@style/TableResult">

        <View
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:layout_marginVertical="4dp"
            android:layout_weight="10"
            android:background="@color/black" />
    </TableRow>
</TableLayout>